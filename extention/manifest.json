{
    "name":"obj ext",
    "description":"my ext",
    "version":"0.1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"./background.js",
        "type":"module"
    },
    "options_page":"./options.html",
    "action":{
        "default_popup":"popup.html"
    },
    "host_permissions":[
        "https://www.google.com/*"
    ],
    "permissions":[
        "tabs",
        "scripting"
    ]
}